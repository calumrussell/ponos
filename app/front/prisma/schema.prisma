generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model match {
  id            Int      @id
  home_id       Int
  away_id       Int
  start_date    Int
  broken        Boolean?
  tournament_id Int?     @db.SmallInt
