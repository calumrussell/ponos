generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model match {
  id         Int      @id
  home_id    Int
  away_id    Int
  start_date Int
  broken     Boolean?
}

model player {
  id   Int     @id
  name String?
}

model player_stats {
  player_id                 Int
  team_id                   Int
  match_id                  Int
  minutes                   Int
  pass                      Int @db.SmallInt
  pass_corner               Int @db.SmallInt
  pass_longball             Int @db.SmallInt
  pass_cross                Int @db.SmallInt
  pass_back                 Int @db.SmallInt
  pass_forward              Int @db.SmallInt
  pass_left                 Int @db.SmallInt
  pass_right                Int @db.SmallInt
  pass_short                Int @db.SmallInt
  pass_throughball          Int @db.SmallInt
  pass_accurate             Int @db.SmallInt
  pass_short_accurate       Int @db.SmallInt
  pass_corner_accurate      Int @db.SmallInt
  pass_longball_accurate    Int @db.SmallInt
  pass_cross_accurate       Int @db.SmallInt
  pass_throughball_accurate Int @db.SmallInt
  pass_key                  Int @db.SmallInt
  pass_key_cross            Int @db.SmallInt
  pass_key_freekick         Int @db.SmallInt
  pass_key_corner           Int @db.SmallInt
  pass_key_throughball      Int @db.SmallInt
  shot                      Int @db.SmallInt
  shot_on_target            Int @db.SmallInt
  shot_off_target           Int @db.SmallInt
  shot_blocked              Int @db.SmallInt
  shot_open_play            Int @db.SmallInt
  shot_set_piece            Int @db.SmallInt
  shot_on_post              Int @db.SmallInt
  shot_six_yard_box         Int @db.SmallInt
  shot_penalty_area         Int @db.SmallInt
  shot_box                  Int @db.SmallInt
  shot_counter              Int @db.SmallInt
  shot_head                 Int @db.SmallInt
  shot_foot                 Int @db.SmallInt
  shot_0bp                  Int @db.SmallInt
  goal                      Int @db.SmallInt
  goal_normal               Int @db.SmallInt
  goal_head                 Int @db.SmallInt
  goal_foot                 Int @db.SmallInt
  goal_set_piece            Int @db.SmallInt
  goal_own                  Int @db.SmallInt
  goal_counter              Int @db.SmallInt
  goal_open_play            Int @db.SmallInt
  goal_0bp                  Int @db.SmallInt
  goal_0box                 Int @db.SmallInt
  goal_six_yard_box         Int @db.SmallInt
  goal_penalty_area         Int @db.SmallInt
  assist                    Int @db.SmallInt
  assist_cross              Int @db.SmallInt
  assist_corner             Int @db.SmallInt
  assist_throughball        Int @db.SmallInt
  aerial_duel               Int @db.SmallInt
  red_card                  Int @db.SmallInt
  yellow_card               Int @db.SmallInt
  second_yellow_card        Int @db.SmallInt
  save                      Int @db.SmallInt
  duel                      Int @db.SmallInt
  duel_offensive            Int @db.SmallInt
  duel_defensive            Int @db.SmallInt
  dispossessed              Int @db.SmallInt
  turnover                  Int @db.SmallInt
  dribble                   Int @db.SmallInt
  dribble_won               Int @db.SmallInt
  dribble_lost              Int @db.SmallInt
  dribble_last_man          Int @db.SmallInt
  challenge_lost            Int @db.SmallInt
  blocked_cross             Int @db.SmallInt
  block_outfielder          Int @db.SmallInt
  block_six_yard            Int @db.SmallInt
  block_pass_outfielder     Int @db.SmallInt
  interception              Int @db.SmallInt
  interception_won          Int @db.SmallInt
  interception_in_box       Int @db.SmallInt
  tackle                    Int @db.SmallInt
  tackle_won                Int @db.SmallInt
  tackle_lost               Int @db.SmallInt
  tackle_last_man           Int @db.SmallInt
  offside_given             Int @db.SmallInt
  offside_provoked          Int @db.SmallInt
  ball_recovery             Int @db.SmallInt
  clearance                 Int @db.SmallInt
  clearance_effective       Int @db.SmallInt
  clearance_off_line        Int @db.SmallInt
  error_leads_to_goal       Int @db.SmallInt
  error_leads_to_shot       Int @db.SmallInt
  touch                     Int @db.SmallInt
  penalty_won               Int @db.SmallInt
  penalty_conceded          Int @db.SmallInt
  penalty_scored            Int @db.SmallInt
  big_chance_missed         Int @db.SmallInt
  big_chance_scored         Int @db.SmallInt
  big_chance_created        Int @db.SmallInt
  parried_safe              Int @db.SmallInt
  parried_danger            Int @db.SmallInt
  save_keeper               Int @db.SmallInt

  @@id([player_id, match_id])
}

